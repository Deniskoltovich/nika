nrel_collected_by
<- sc_node_norole_relation;  /* Это неролевое отношение */
<- relation;  /* Это отношение */
<- binary_relation;  /* Бинарное отношение, связывающее произведение искусства с галереей, которая его собрала */
<- oriented_relation;  /* Ориентированное отношение: галерея собирает произведение, а не наоборот */
<- antireflexive_relation;  /* Произведение не может быть собрано само собой */
<- asymmetric_relation;  /* Если галерея собрала произведение, то произведение не может собрать галерею */
=> nrel_main_idtf:
   [собрано галереей]
   (* <- lang_ru;; *);
=> nrel_first_domain: concept_artwork;  /* Первое множество — произведения искусства */
=> nrel_second_domain: concept_gallery;;  /* Второе множество — галереи */
